package cicle;

import java.util.Scanner;

public class LolerNum {

	
	public static void main(String[] args) {
		System.out.println("Enter how many number you want to compare:");
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int biggest=0;
		 for (int i=0; i<x; i++){
		System.out.println("tipe the number");
	       int a=sc.nextInt();
	       if(biggest>a){
	    	   biggest=a;
	       }
		 }
		 System.out.println("The biggest is: "+biggest);

	}

}

