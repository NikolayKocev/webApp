package cicle;

import java.util.Scanner;

public class Fibonaci {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	 int x=1;
	 int y=2;
	 int z=0;
	 Scanner sc = new Scanner(System.in);
	 System.out.println("enter a number");
	 int n = sc.nextInt();
	 for (int i = 2; i<n; i++){
		 z=x;
		 x=x+y;
		 y=z;
	 }
	 System.out.println(x);
	}

}

